    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Local Load",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "env": {
                "STORAGE": "AZURE_BLOB",
                "BACKGROUND_DOWNLOAD": "DISABLED"
            }
        },
        {
            "name": "Launch Azure Load",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "env": {
                "STORAGE": "AZURE_BLOB"
            }
        },
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 40000,
            "host": "127.0.0.1"
        }
    ]
}